{
  "version": 2,
  "env": {
    "APP_URL": "https://k-service.vercel.app"
  },
  "builds": [
    {
      "src": "public/index.php",
      "use": "vercel-php@0.6.2"
    },
    {
      "src": "public/build/**",
      "use": "@vercel/static"
    },
    {
      "src": "public/images/**",
      "use": "@vercel/static"
    },
    {
      "src": "public/favicon.ico",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/public/build/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.php"
    }
  ]
}